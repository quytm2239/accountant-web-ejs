<%# <nav class="sidebar sidebar-offcanvas" id="sidebar"> %>
<nav class="sidebar" id="sidebar">
	<ul class="nav">
		<li class="nav-item nav-profile">
			<div class="nav-link">
				<div class="user-wrapper">
					<div class="profile-image">
						<img src="<%= profile.avatar%>" alt="profile image">
					</div>
					<div class="text-wrapper">
						<p class="profile-name"><%= profile.full_name %></p>
						<div>
							<small class="designation text-muted"><%= profile.job_title%></small>
							<span class="status-indicator online"></span>
						</div>
					</div>
				</div>
				<button class="btn btn-success btn-block" id="testDelete">New<i class="mdi mdi-plus"></i></button>
			</div>
		</li>
		<% funcItem.forEach(function(item) { %>
			<li class="nav-item">
				<a class="nav-link" href="<%= item.path %>">
					<span class="menu-title"><%= item.name %></span>
				</a>
			</li>
		<% }) %>
	</ul>
</nav>
