<div class="row">
	<div class="col-lg-12 grid-margin stretch-card">
		<div class="card">
			<div class="card-body">
				<h4 class="card-title">Danh sách hồ sơ chờ phê duyệt</h4>
				<%# <p class="card-description">
					Add class
					<code>.table-striped</code>
				</p> %>
				<div class="table-responsive">
					<table class="table table-bordered">
						<thead>
							<tr>
								<% listHeader.forEach(header => { %>
									<th><%= header %></th>
								<% }) %>
							</tr>
						</thead>
						<tbody>
							<% pendingList.forEach(elem => { %>
							<tr>
								<% listHeader.forEach(header => { %>
									<% if (header == 'avatar') { %>
										<td class="py-1"><img src="<%= elem[header] %>" alt="image" /></td>
									<% } else { %>
										<td><%= elem[header] %></td>
									<% } %>
								<% }) %>
							</tr>
							<% }) %>
						</tbody>
					</table>
				</div>
			</div>
		</div>
	</div>
</div>
